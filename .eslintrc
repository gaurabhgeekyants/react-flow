
{
    "parser": "babel-eslint",
    "extends": [ "airbnb"],
    "rules": {
        "block-scoped-var": 1,
        "class-methods-use-this": 1,
        "import/prefer-default-export": 1,
        "max-len": [1, 160, 4],
        "no-multi-assign": 1,
        "no-param-reassign": 1,
        "no-plusplus": 1,
        "no-proto": 1,
        "no-prototype-builtins": 1,
        "no-restricted-syntax": 1,
        "no-return-assign": 1,
        "no-shadow": 1,
        "no-sequences": 1,
        "no-underscore-dangle": 1,
        "no-unused-vars": 1,
        "no-unused-expressions": 1,
        "no-use-before-define": 1,
        "no-useless-concat": 1,
        "no-var": 1,
        "no-void": 1,
        "one-var": 1,
        "prefer-rest-params": 1,
        "prefer-const": 1,
        "vars-on-top": 1
    }
}
